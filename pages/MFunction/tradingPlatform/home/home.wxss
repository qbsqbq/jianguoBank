.mapView {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 49px;
  left: 0;
}
.map {
  width: 100%;
  height: 100%;
}
.tabbar {
  position: absolute;
  display: flex;
  background: #909490;
  height: 49px;
  right: 0;
  bottom: 0;
  left: 0;
}
.btn {
  flex: 1;
  background: #00CED1;
  color: white;
  line-height: 49px;
  text-align: center;
  font-size: 15px;
}
.release {
  background: #EE9A00;
}